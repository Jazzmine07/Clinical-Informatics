        $("#medicationVisit").change(function(){
            document.getElementById('validAmount').style.display = "none";
            document.getElementById('invalidAmount').style.display = "none";
            var selectedMedication = $('#medicationVisit').find('option:selected').val();
            var medicationTxt = $('#medicationVisit').find('option:selected').text();
            var lowerCase = selectedMedication.toLowerCase();

            console.log("not allowed medications of student");
            console.log(notAllowedMedicines);
            var i;
            if(notAllowedMedicines.length == 0){
                // if not allowed medicines is empty, allow all medications
                $('span.select2-selection.select2-selection--single').css('border', '1px solid #1cc88a');
                document.getElementById('invalidMedicaton').style.display = "none";
                document.getElementById('validMedicaton').style.display = "block";
                $('#validMedicaton').html('Medication allowed'); 
            } else{
                for(i = 0; i < notAllowedMedicines.length; i++){           
                    if(lowerCase.includes(notAllowedMedicines[i].medicine) == true){
                        $('span.select2-selection.select2-selection--single').css('border-color', '#e74a3b');  
                        document.getElementById('validMedicaton').style.display = "none";
                        document.getElementById('invalidMedicaton').style.display = "block";
                        $('#invalidMedicaton').html('Medication not allowed');       
                        break;     
                    }
                    else{
                        $('span.select2-selection.select2-selection--single').css('border', '1px solid #1cc88a');
                        document.getElementById('invalidMedicaton').style.display = "none";
                        document.getElementById('validMedicaton').style.display = "block";
                        $('#validMedicaton').html('Medication allowed');    
                    }
                }
            }

            var curWeight = document.getElementById("weight").value;
            var x; //could be weight or age

            if(curWeight != null && curWeight != "" && curWeight != " " && curWeight != undefined){
                console.log("weight: " + curWeight);
                x = curWeight;
            }
            else if(weight != null && weight != ""){
                console.log("data.weight: " + weight);
                x = parseFloat(weight);
            }
            else{
                x = 1;
            } 

            console.log("x is...." + x);

            if(medicationTxt == "Paracetamol Syrup 250mg/5mL"){
                if(x != 1){
                    var start = x * 10;
                    var wbd = start / (250/5); 
                    console.log("weight-based: " + wbd);                          
                    document.getElementById('invalidAmount').style.display = "none";
                    document.getElementById('validAmount').style.display = "block";
                    $('#validAmount').html('Weight-based dosage: ' + wbd + 'mL');                       
                } 
            }
            else if(medicationTxt == "Paracetamol Syrup 120mg/5mL"){
                if(x != 1){
                    var start = x * 10;
                    var wbd = start / (120/5); 
                    console.log("weight-based: " + wbd);                          
                    document.getElementById('invalidAmount').style.display = "none";
                    document.getElementById('validAmount').style.display = "block";
                    $('#validAmount').html('Weight-based dosage: ' + wbd + 'mL');                       
                } 
            }
            else if(medicationTxt == "Paracetamol Suspension 250mg/5mL"){
                if(x != 1){
                    var start = x * 10;
                    var wbd = start / (250/5); 
                    console.log("weight-based: " + wbd);                          
                    document.getElementById('invalidAmount').style.display = "none";
                    document.getElementById('validAmount').style.display = "block";
                    $('#validAmount').html('Weight-based dosage: ' + wbd + 'mL');                       
