        async function getInteraction(){
            descArray = [];
            list = ids.toString().replace(/,/g, "+");
            const response = await fetch("https://rxnav.nlm.nih.gov/REST/interaction/list.json?rxcuis="+list);
            const data = await response.json();
            
            if(data.fullInteractionTypeGroup != undefined){
                data.fullInteractionTypeGroup.forEach(function(interactionTypeGroup) {
                    interactionTypeGroup.fullInteractionType.forEach(function(fullInteractionType){
                        fullInteractionType.interactionPair.forEach(function(description){
                            descArray.push(description.description);
                            console.log(descArray);
                        })
                    })
                })
                
                tempDescArray = descArray.toString();
                tempDescArray = tempDescArray.split(",");

                for (let i = 0; i < descArray.length; i++) {
                tempDescArray[i] = descArray[i] + "<br><br>";
                }

                tempDescArray = tempDescArray.join("");
                console.log(tempDescArray)
                document.getElementById("interactionDiv").innerHTML = tempDescArray;
                document.getElementById("successMed").style.display = "none";
                document.getElementById("errorMed").style.display = "block";
            } else {
                tempDescArray = "No drug-to-drug interaction found.";
                console.log(tempDescArray);
                document.getElementById("noInteractionDiv").innerHTML = tempDescArray;
                document.getElementById("errorMed").style.display = "none";
                document.getElementById("successMed").style.display = "block";
            }
        }

