        function showDailyVisits(data,choice){
            console.log("Hello");
            console.log(data);
            //used to check the current month and year to show
            var curr =  new Date();
            var currMonth = curr.getMonth();
            var currYear = curr.getFullYear();
            //find the day of the week the day is
            var chosenDay, chosenDate;

            var i;
            var days=[], daysString=[], daysCount=[],day;
            var dataDate, dataDay;

        //------------------IF USER WANTS TO SEE COUNT FOR EACH DATE------------------
            if(choice == 0){
                //get the number of days based on the month and year
                if(currMonth == "0" || currMonth == "2" || currMonth == "4" || currMonth == "6" || currMonth == "7" || currMonth == "9" ||currMonth == "11" ){
                    for(i=0;i<31;i++){
                        chosenDate= new Date(currYear,currMonth,i+1);
                        chosenDay = chosenDate.getDay();
                        console.log(chosenDate);
                        days.push({
                            date: i+1,
                            day: chosenDay,
                            count:parseInt("0")
                        });
                    }
                }
                else if(currMonth == "3" ||currMonth == "5" ||currMonth == "8" ||currMonth == "10"){
                    for(i=0;i<30;i++){
                        chosenDate= new Date(currYear,currMonth,i+1);
                        chosenDay = chosenDate.getDay();
                        days.push({
                            date: i+1,
                            day: chosenDay,
                            count:parseInt("0")
                        });
                    }
                }
                else if(currMonth == "1"){
                    if(currYear/4 == 0){
                        for(i=0;i<29;i++){
                            chosenDate= new Date(currYear,currMonth,i+1);
                            chosenDay = chosenDate.getDay();
                            days.push({
                                date: i+1,
                                day: chosenDay,
                                count:parseInt("0")
                            });
                        }
                    }
                    else{
                        for(i=0;i<28;i++){
                            chosenDate= new Date(currYear,currMonth,i+1);
                            chosenDay = chosenDate.getDay();
                            days.push({
                                date: i+1,
                                day: chosenDay,
                                count:parseInt("0")
                            });
                        }
                    }
                };
                    //loops if to get the count for each day
                    for(i=0;i<data.length;i++){
                        for(j=0;j<days.length;j++){ 
                            if(data[i].complaint!="" && data[i].complaint!=null && data[i].complaint!=undefined){ 
                                dataDate= new Date(data[i].visitDate);
                                dataDay = dataDate.getDate();
                                if(dataDay==days[j].date-1){
                                    days[j].count=days[j].count+1;
                                }
                            }
                        }
                    }
                    //loop is used to push data to daysString for the label and daysCount for the data 
                    for(i=0;i<days.length;i++){
                    if(days[i].day == 0)
                        day="Sun";
                    else if(days[i].day == 1)
                        day="Mon";
                    else if(days[i].day == 2)
                        day="Tue";
                    else if(days[i].day == 3)
                        day="Wed";
                    else if(days[i].day == 4)
                        day="Thu";
                    else if(days[i].day == 5)
                        day="Fri";
                    else if(days[i].day == 6)
                        day="Sat";
                        
                        daysString.push(days[i].date + ", " + day);
                        daysCount.push(days[i].count);
                    }  
                
            }
                           
        //------------------IF USER WANTS TO SEE COUNT FOR EACH DAY------------------
            else if(choice ==1){
                daysString.push("Sunday");
                daysString.push("Monday"); 
                daysString.push("Tuesday"); 
                daysString.push("Wednesday"); 
                daysString.push("Thursday"); 
                daysString.push("Friday"); 
                daysString.push("Saturday"); 
                daysCount=[0,0,0,0,0,0,0];
                for(i=0;i<data.length;i++){
                    if(data[i].complaint!="" && data[i].complaint!=null && data[i].complaint!=undefined){ 
                        dataDate= new Date(data[i].visitDate);
                        dataDay = dataDate.getDate();
                        day = dataDate.getDay();
                        console.log(day);
                        if(day == 0)
