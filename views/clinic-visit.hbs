                        else if(day == 3)
                            daysCount[3]=daysCount[3]+1;
                        else if(day == 4)
                            daysCount[4]=daysCount[4]+1;
                        else if(day == 5)
                            daysCount[5]=daysCount[5]+1;
                        else if(day == 6)
                            daysCount[6]=daysCount[6]+1;
                    }
                }

            }
        //------------------END OF "IF USER WANTS TO SEE COUNT FOR EACH DAY"------------------
            
           
            var chartData={
                labels: daysString, // x-axis
                datasets: [{
                    label: "Visit Count",
