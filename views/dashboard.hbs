            //find the day of the week the day is
            var chosenDay, chosenDate;

            var i;
            var days=[], daysString=[], daysCount=[],day;
            var dataDate, dataDay;

        //------------------IF USER WANTS TO SEE COUNT FOR EACH DATE------------------
