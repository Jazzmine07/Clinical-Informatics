                                        <label class="form-check-label" for="flexCheckChecked">Essentially Normal P.E.</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Basic Card Example -->
                    <div class="card shadow mb-2" id="scrollHere">
                        <div class="card-header py-3 d-sm-flex">
                            <h6 class="m-0 font-weight-bold text-primary mr-auto p-2">S.Y. <span id="schoolYear"></span>  |  <span id="sectionTop"></span></h6>                           
                            
                            <button type="button" class="btn btn-primary btn-sm mr-2 mx-1" id="prevBtn"><i
                                class="fas fa-backward mr-2 text-white-50" ></i>Previous</button>
                            <div class="dropdown">
                                <button id="curStudent" style="height: 100%" onclick="myFunction()" class="btn btn-primary btn-sm mr-2 mx-1">Current Student</button>
                                <div id="myDropdown" class="dropdown-content dropdown-menu dropdown-menu-right">
                                    <input type="text" placeholder="Search.." id="myInput" onkeyup="filterFunction()">
                                    
                                </div>
                            </div>    
                            <button type="button" class="btn btn-primary btn-sm mx-1" id="skipBtn">Skip<i
                                class="fas fa-forward ml-2 text-white-50"></i></button>
                        </div>
                        <div class="card-body">
                            
                                <div class="form-row align-items-center">
                                    <div class="col-4">
                                      <label class="col-form-label" for="studentID">Student ID Number</label>
                                      <input type="text" class="form-control mb-2" id="studentID" name="studentId" disabled="true">
                                    </div>
                                    <div class="col-5">
                                        <label class="col-form-label" for="studentName">Student Name</label>
                                        <input type="text" class="form-control mb-2" id="studentName" name="studentName" disabled="true">
                                    </div>
                                </div>

                                <div class="form-row align-items-center">
                                    <div class="col-4">
                                        <label for="studentGrade" class="col-form-label">Grade</label>
                                        <select class="custom-select form-control mb-2" id="studentGrade"disabled="true">
                                            <option disabled="disabled" selected="true">Choose...</option>
                                            <option value="1">One</option>
                                            <option value="2">Two</option>
                                            <option value="3">Three</option>
                                            <option value="4">Four</option>
                                            <option value="5">Five</option>
                                            <option value="6">Six</option>
                                        </select>
                                    </div>
                                    <div class="col-5">
                                        <label for="studentSection" class="col-form-label">Section</label>
                                        <input type="text" class="form-control mb-2" id="studentSection" disabled="true">
                                    </div>
                                </div>

                                <div class="form-row align-items-center">
                                    <div class="col-4">
                                        <label for="dob" class="col-form-label">Date of Birth</label>
                                        <input type="date" class="form-control mb-2" id="dob" disabled="true">
                                    </div>
                                    <div class="col-3">
                                        <label for="gender" class="col-form-label">Gender</label>
                                        <input type="name" class="form-control mb-2" id="gender" disabled="true">
                                    </div>
                                    <div class="col-2">
                                        <label for="age" class="col-form-label">Age</label>
                                        <input type="number" class="form-control mb-2" id="age" disabled="true">
                                    </div>
                                </div>

                                <div class="form-row align-items-center">
                                    <div class="col-4">
                                        <label for="dope" class="col-form-label">Date of Examination</label>
                                        <input type="date" class="form-control mb-2" id="dope" disabled="true">
                                    </div>
                                    <div class="col-5">
                                      <label class="col-form-label" for="clinicianAPE">Examining Physician</label><label style="color:red">*</label>
                                        <select class="custom-select mb-2" id="clinicianAPE" name="clinicianAPE">
                                            <option selected>Choose...</option>
                                            {{#each clinician}}
                                                <option clinician="{{firstName}} {{lastName}}" value="{{key}}">{{firstName}} {{lastName}}</option>
                                            {{/each}}
                                        </select>
                                    </div>   
                                </div>

                                <div class="form-row align-items-center">
                                    <div class="col-2">
                                        <label for="bodyTemp" class="col-form-label">Body Temperature</label><label style="color:red">*</label>
                                        <div class="input-group mb-2">                                        
                                            <input type="number" class="form-control" id="bodyTemp" name="bodyTemp" min="0" step="0.1">                                        
                                            <div class="input-group-append">
                                                <span class="input-group-text" id="basic-addon1">°C</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-3">
                                        <label for="bloodPressure" class="col-form-label">Blood Pressure</label><label style="color:red">*</label>
                                        <div class="input-group mb-2">                               
                                            <input type="number" class="form-control" id="systolic" placeholder="Systolic" min="0">
                                            <input type="number" class="form-control" id="diastolic" placeholder="Diastolic" min="0">         
                                            <div class="input-group-append">
                                                <span class="input-group-text" id="basic-addon2">mmHg</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-2">
                                        <label for="pulseRate" class="col-form-label">Pulse Rate</label><label style="color:red">*</label>
                                        <div class="input-group mb-2">                                        
                                            
                                        <input type="number" class="form-control" id="pulseRate" name="pulseRate" min="0">
                                            <div class="input-group-append">
                                                <span class="input-group-text" id="basic-addon3" min="0">beats/min</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-2">
                                        <label for="respRate" class="col-form-label">Respiration Rate</label><label style="color:red">*</label>
                                        <div class="input-group mb-2">                                        
                                            
                                        <input type="number" class="form-control" id="respRate" name="respRate">
                                            <div class="input-group-append">
                                                <span class="input-group-text" id="basic-addon4" min="0">breaths/min</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-row align-items-center">
                                    <div class="col-2" style="height: 24px;">
                                        <div id="bodyTempValid" class="valid-feedback status font-weight-bold"></div>
                                        <div id="bodyTempInvalid" class="invalid-feedback status font-weight-bold"></div>
                                    </div>
                                    <div class="col-1" style="height: 24px;">
                                        <div id="normalSystolic" class="valid-feedback status font-weight-bold">Normal</div>
                                        <div id="belowSystolic" class="invalid-feedback status font-weight-bold">Below Normal</div>
                                        <div id="aboveSystolic" class="invalid-feedback status font-weight-bold">Above Normal</div>
                                    </div>
                                    <div class="col-2" style="height: 24px; text-align: center; padding-left: 30px;">
                                        <div id="normalDiastolic" class="valid-feedback status font-weight-bold">Normal</div>
                                        <div id="belowDiastolic" class="invalid-feedback status font-weight-bold">Below Normal</div>
                                        <div id="aboveDiastolic" class="invalid-feedback status font-weight-bold">Above Normal</div>
                                    </div>      
                                    <div class="col-2" style="height: 24px;">
                                        <div id="normalPulseRate" class="valid-feedback status font-weight-bold">Normal</div>
                                        <div id="belowPulseRate" class="invalid-feedback status font-weight-bold">Below Normal</div>
                                        <div id="abovePulseRate" class="invalid-feedback status font-weight-bold">Above Normal</div>
                                    </div>
                                    <div class="col-2" style="height: 24px;">
