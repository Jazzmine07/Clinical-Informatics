                    <!-- Basic Card Example -->
                    <div class="card shadow mb-2">
                        <div class="card-header py-3 d-sm-flex">
                            <h6 class="m-0 font-weight-bold text-primary mr-auto p-2">S.Y. <span id="schoolYear"></span>  |  <span id="sectionTop"></span></h6>                           
                            
                            <button type="button" class="btn btn-primary btn-sm mr-2 mx-1" id="prevBtn"><i
                                class="fas fa-backward mr-2 text-white-50" ></i>Previous</button>
                            <button type="button" class="btn btn-primary btn-sm mr-2 mx-1"><i
                                class="fas fa-download mr-2 text-white-50"></i>Load Old Record</button>
