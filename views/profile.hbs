                console.log(data);
                var i;

                if(data.length != 0){
                    $("#dynamicIntake tr").remove();
                        for(i = 0; i < data.length; i++){
                            let visit = new Date(data[i].visitDate)
                            let visitMonth = visit.toLocaleString('default', { month: 'long' })
                            let visitDate = visitMonth + ' ' + visit.getDate() + ', ' + visit.getFullYear();
                        $('#dynamicIntake').append('<tr><td>' + data[i].medicine + '</td><td>' + data[i].amount + '</td><td>' + visitDate + '</td><td>' + data[i].time + '</td></tr>');
                    }
                }   
                else{
                    $("#dynamicIntake tr").remove();
                    $('#dynamicIntake').append('<tr><td colspan="4" class="font-weight-normal text-center">' + "No data available" + '</td></tr>');   
                }

            });

            // --------------------------------- LIST OF PRESCRIPTION HISTORY ------------------------------
            $.get("/getPrescriptionHistory", {studentID: typedID}, function(data){
                console.log("prescription history of a specfic student");
                console.log(data);
                var i;

                var startDateMed, endDateMed;

                // TO DO: add checker if may prescribed by, kasi pwede parent mag input dito so baka malimutan ilagay ng parent sino nag prescribe
                if(data.length != 0){
                    $("#dynamicPrescription tr").remove();
                        for(i = 0; i < data.length; i++){
                            if(data[i].startDate != ""){
                                let startDate = new Date(data[i].startDate)
                                let startDateMonth = startDate.toLocaleString('default', { month: 'long' })
                                startDateMed = startDateMonth + ' ' + startDate.getDate() + ', ' + startDate.getFullYear();
                            }
                            else {
                                startDateMed = "";
                            }
                                

                            if(data[i].endDate != ""){
                                let endDate = new Date(data[i].endDate)
                                let endDateMonth = endDate.toLocaleString('default', { month: 'long' })
                                endDateMed = endDateMonth + ' ' + endDate.getDate() + ', ' + endDate.getFullYear();
                            }
                            else {
                                endDateMed = "";
                            }
                                
                        $('#dynamicPrescription').append('<tr><td>' + data[i].medicine + '</td><td>' + data[i].purpose + '</td><td>' + data[i].amount + '</td><td>' + data[i].interval + '</td><td>' + startDateMed + '</td><td>' + endDateMed + '</td><td>' + data[i].status + '</td></tr>');
                    }
                }   
                else{
                    $("#dynamicPrescription tr").remove();
                    $('#dynamicPrescription').append('<tr><td colspan="7" class="font-weight-normal text-center">' + "No data available" + '</td></tr>');   
                }
            
            });

