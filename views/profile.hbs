                    }
                    else if(data[i].medicine == "decolgen" && data[i].isAllowed == true){
                        document.getElementById("decolgen").checked = true;
                    }
                    else if(data[i].medicine == "epinephrine" && data[i].isAllowed == true){
                        document.getElementById("epinephrine").checked = true;
                    }
                    else if(data[i].medicine == "hydrite" && data[i].isAllowed == true){
                        document.getElementById("hydrite").checked = true;
                    }
                    else if(data[i].medicine == "ibuprofen" && data[i].isAllowed == true){
                        document.getElementById("ibuprofen").checked = true;
                    }
                    else if(data[i].medicine == "loratidine" && data[i].isAllowed == true){
                        document.getElementById("loratidine").checked = true;
                    }
                    else if(data[i].medicine == "mefenamic" && data[i].isAllowed == true){
                        document.getElementById("mefenamic").checked = true;
                    }
                    else if(data[i].medicine == "neozep" && data[i].isAllowed == true){
                        document.getElementById("neozep").checked = true;
                    }
                    else if(data[i].medicine == "paracetamol" && data[i].isAllowed == true){
                        document.getElementById("paracetamol").checked = true;
                    }
                    else if(data[i].medicine == "salbutamol" && data[i].isAllowed == true){
                        document.getElementById("salbutamol").checked = true;
                    }
                    else if(data[i].medicine == "lastUpdated"){
                        document.getElementById("medLastUpdated").innerHTML = "Last updated: " + data[i].lastUpdated;
                    }
                }
            });

            // --------------------------------- UPLOADS ------------------------------
            $.get("/getUploads", {studentID: typedID}, function(data){
                console.log("med cert and/or lab results of a specfic student");
                console.log(data);
                var i;

                console.log("medcerts");
                console.log(data.medCert);

                if(data.length != 0){
                    var medCertBody = document.getElementById("medCertBody");
                    medCertBody.innerHTML = "";

                    var labResultBody = document.getElementById("labResultBody");
                    labResultBody.innerHTML = "";

                    for(var i = 0; i < data.medCert.length; i++){
                        let date = new Date(data.medCert[i].dateUploaded)
                        let month = date.toLocaleString('default', { month: 'long' })
                        let medDate = month + ' ' + date.getDate() + ', ' + date.getFullYear();
                        $('#medCertBody').append('<a href="'+data.medCert[i].url+'" target="_blank" class="d-block card-header py-3 m-0 font-weight-bold text-primary" role="button">' + medDate + '</a>');
                    }

                    for(var i = 0; i < data.labResults.length; i++){
                        let date = new Date(data.labResults[i].dateUploaded)
                        let month = date.toLocaleString('default', { month: 'long' })
                        let labDate = month + ' ' + date.getDate() + ', ' + date.getFullYear();
                        $('#labResultBody').append('<a href="'+data.labResults[i].url+'" target="_blank" class="d-block card-header py-3 m-0 font-weight-bold text-primary" role="button">' + labDate + '</a>');
                    }
                }
                else{
                    var medCertBody = document.getElementById("medCertBody");
                    medCertBody.innerHTML = "";

                    var labResultBody = document.getElementById("labResultBody");
                    labResultBody.innerHTML = "";
                }
            });
        }

