                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title" id="modalLongTitle">Schedule for Physical and Dental Exam</h5>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <div class="modal-body">
                                    <div class="form-group">
                                        <label for="schoolYear">Enter School Year</label>                                        
                                        <div class="input-group mb-3">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text" id="basic-addon1">S.Y.</span>
                                        </div>
                                        <input type="text" class="form-control" id="schoolYear" placeholder="YYYY-YYYY" aria-label="school-year" aria-describedby="basic-addon1">
                                        </div>
                                        <label for="sectionPE">Enter Grade and Section</label>
                                        <div class="input-group">
                                            <span class="input-group-prepend">
                                                <div class="input-group-text bg-transparent border-right-0">
                                                <i class="fa fa-search"></i>
                                                </div>
                                            </span>
                                            <input list="sections" class="form-control py-2 border-left-0 border" id="sectionPE" placeholder="Search by grade or section"/>                                       
                                        </div>
                                        <datalist id="sections">
                                            <option value="Grade 1 - Faithfulness">
                                            <option value="Grade 1 - Honesty">
                                            <option value="Grade 1 - Humility">
                                            <option value="Grade 1 - Politeness">
                                            <option value="Grade 1 - Sincerity">
                                            <option value="Grade 2 - Charity">
                                            <option value="Grade 2 - Gratefulness">
                                            <option value="Grade 2 - Gratitude">
                                            <option value="Grade 2 - Meekness">
                                            <option value="Grade 2 - Simplicity">                                
                                        </datalist>  
